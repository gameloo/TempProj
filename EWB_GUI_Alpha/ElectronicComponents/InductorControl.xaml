<UserControl
    x:Class="EWB_GUI_Alpha.ElectronicComponents.InductorControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:EWB_GUI_Alpha.ElectronicComponents"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="100"
    d:DesignWidth="100">

    <Path Stroke="Black">
        <Path.Data>
            <PathGeometry>
                <PathFigure StartPoint="0,50">
                    <LineSegment Point="20,50"/>
                </PathFigure>
                <PathFigure StartPoint="80,50">
                    <LineSegment Point="100,50"/>
                </PathFigure>
                <PathFigure StartPoint="40,50">
                    <ArcSegment Point="20,50" Size="25,100"/>
                </PathFigure>
                <PathFigure StartPoint="60,50">
                    <ArcSegment Point="40,50" Size="25,100"/>
                </PathFigure>
                <PathFigure StartPoint="80,50">
                    <ArcSegment Point="60,50" Size="25,100"/>
                </PathFigure>
            </PathGeometry>
        </Path.Data>
    </Path>
</UserControl>
